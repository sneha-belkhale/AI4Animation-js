{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "jsx-a11y/click-events-have-key-events": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "global-require": 0,
    "no-dynamic-require": 0,
    "no-await-in-loop": 0
  }
}
